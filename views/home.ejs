  <body>
    <%- include('includes/header.ejs') %>

    <main>
      <h1>Home</h1>

      
        <% for (let index of memories.keys()) { %>
          <div class="memory">
          <h3>Memory <%= index +1 %></h3>
          <p><strong><%= memories[index].title %></strong></p>
          <p>GPS Posicion: <%= memories[index].gps %></p>
          <img src="<%= memories[index].imageUrl %>" alt="" srcset="">
          <p>Comment:<%= memories[index].comment %></p>

          </div>
        <% } %>
      
    </main>

