  <%- include('includes/head.ejs') %>
  
  </head>
  <body>
    <%- include('includes/header.ejs') %>

    <main>
      <h1>Home</h1>
        <div class="memories">
          <% for (let index of memories.keys()) { %>
            <div class="memory">
              <h2><%= memories[index].title %></h2>
              <img src="<%= memories[index].imageUrl %>" alt="" srcset="">
              <p><i>GPS Posicion: <%= memories[index].gps %></i></p>
              <h5><%= memories[index].comment %></h5>
              <div class="buttons">
                <a href="/detail-memory/<%= memories[index].id %>" class="link-button">View</a>
                <a href="/edit-memory/<%= memories[index].id %>" class="link-button">Edit</a>
              </div>
            </div>
          <% } %>
        </div>
      
    </main>

    <%- include('includes/footer.ejs') %>